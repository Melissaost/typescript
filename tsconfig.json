{
    "compilerOptions": {
        //onde vai salvar o javascript 
        "outDir": "dist/js",
        //qual a versao que vai escrever o js
        "target": "ES6",
        //nao criar pasta js se tiver erro
        "noEmitOnError": true,
        //nao adotar valor any nas variaveis 
        "noImplicitAny": true
    },
    //onde esta o ts
    "include": [
        "app/**/*"
    ]
}